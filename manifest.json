{
  "name": "MHX Wiki Translator",
  "short_name": "MHX-Wiki-Tr",
  "description": "Translate armor skill tree names and other data on http://wiki.mhxg.org, http://game-cap.com/, and http://mhx-wiki.com/.",
  "version": "0.3",
  "homepage_url": "https://github.com/bd4/chrome_mhx_translate",
  "content_scripts": [
    {
      "matches": ["http://wiki.mhxg.org/*",
                  "http://game-cap.com/*",
                  "http://mhx-wiki.com/*"],
      "css": ["override.css"],
      "js": ["mhx_skill_tree_list.js",
             "mhx_monster_replace_list.js",
             "fix_select_and_menu.js",
             "translate_links.js"],
      "run_at": "document_idle"
    }
  ],
  "manifest_version": 2
}
